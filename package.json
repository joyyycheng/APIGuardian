{
  "name": "apiguardian",
  "displayName": "API Guardian",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/joyyycheng/APIGuardian.git"
  },
  "version": "0.0.2",
  "publisher": "API Guardian",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "api.checkAPIStatus",
        "title": "Search for API"
