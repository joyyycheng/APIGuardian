    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "axios": "^1.7.7",
    "fetch-mock": "^12.2.0",
    "generator-code": "^1.11.3",
    "mysql2": "^3.11.4",
    "natural": "^8.0.1",
    "supertest": "^7.0.0",
    "unit.js": "^2.1.1",
